{
    "name": "ytj-snippets",
    "displayName": "Snippets",
    "description": "My personal vscode code snippets",
    "preview": true,
    "version": "0.1.0",
    "publisher": "YuTengjing",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact",
        "react",
        "vue",
        "rust"
    ],
    "repository": {
        "type": "git",
        "url": "git+git@github.com:tjx666/snippets.git"
    },
    "prettier": "@yutengjing/prettier-config",
    "scripts": {
        "release": "bumpp -c \"release: v%s\" package.json",
        "publish:vs-marketplace": "vsce publish --no-dependencies",
        "publish:open-vsx": "ovsx publish --no-dependencies"
    },
    "devDependencies": {
        "@vscode/vsce": "^2.18.0",
        "@yutengjing/prettier-config": "^0.0.1",
        "bumpp": "^9.1.0",
        "ovsx": "^0.8.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/javascript.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/javascript.code-snippets"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/javascript.code-snippets"
            },
            {
                "language": "rust",
                "path": "./snippets/rust.code-snippets"
            }
        ]
    }
}
